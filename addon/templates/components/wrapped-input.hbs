<label for="{{inputId}}">{{labelText}}</label>
{{#if autoFocus}}
	{{textfield-auto-focus value=value placeholder=placeholderText  class="form-control" id=inputId maxlength=maxlength}}
{{else}}
	{{input class="form-control" value=value placeholder=placeholderText id=inputId maxlength=maxlength}}
{{/if}}
